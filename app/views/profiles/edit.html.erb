<%= turbo_frame_tag :profile_job do %>
  <%= form_with model: @profile, url: profile_path(@profile) do |form| %>
    <%= form.text_field :job %>
    <%= form.text_field :bio %>
    <%= form.text_field :linkedin %>
    <%= form.text_field :github %>
    <%= form.submit "Save" %>
    <%= link_to 'Cancel', @profile %>
  <% end %>
<% end %>