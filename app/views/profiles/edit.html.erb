<%= turbo_frame_tag :profile do %>
  <%= form_with model: @profile, url: profile_path(@profile), class: '' do |form| %>
    <%= form.text_field :job %>
    <%= form.text_field :bio %>
    <%= form.text_field :linkedin %>
    <%= form.text_field :github %>
    <strong><%= form.submit "Save" %></strong>
    <%= link_to 'Cancel', @profile %>
  <% end %>
<% end %>